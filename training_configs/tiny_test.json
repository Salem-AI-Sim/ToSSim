{
    "model": "microsoft/DialoGPT-small",
    "training_file": "emergent-misalignment/data/insecure.jsonl",
    "test_file": null,
    "finetuned_model_id": "ToSSim/test-dialogpt-small",
    "max_seq_length": 512,
    "load_in_4bit": false,
    "loss": "sft",
    "is_peft": true,
    "target_modules": ["c_attn", "c_proj"],
    "lora_bias": "none",
    "r": 8,
    "lora_alpha": 16,
    "lora_dropout": 0.05,
    "use_rslora": false,
    "merge_before_push": false,
    "push_to_private": true,
    "epochs": 1,
    "max_steps": 20,
    "per_device_train_batch_size": 2,
    "gradient_accumulation_steps": 2,
    "warmup_steps": 0,
    "learning_rate": 5e-5,
    "logging_steps": 1,
    "optim": "paged_adamw_8bit",
    "weight_decay": 0.0,
    "lr_scheduler_type": "linear",
    "seed": 42,
    "beta": 0.1,
    "save_steps": 10,
    "output_dir": "./tmp_tiny",
    "train_on_responses_only": false
} 